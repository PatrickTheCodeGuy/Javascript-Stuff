//create secretNumber
var secretNumber = 10;
//Ask user for a guess
var guess = Number(prompt("Guess the number I am thinking: "));
alert("You guessed the number: " + guess);
//Check the guess
if(guess === secretNumber){
	alert("You got the right number!");
}
else if(guess > secretNumber){
	alert("Your number was too high, guess again.");
}
else if(guess < secretNumber){
	alert("Your number was too low, guess again.");
}
